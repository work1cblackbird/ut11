	ИначеЕсли ЗначениеПоиска = "GLUED" Тогда
		Результат = Перечисления.ВидыКиЗГИСМ.Клеевой;
	ИначеЕсли ЗначениеПоиска = "HINGED" Тогда
		Результат = Перечисления.ВидыКиЗГИСМ.Навесной;
