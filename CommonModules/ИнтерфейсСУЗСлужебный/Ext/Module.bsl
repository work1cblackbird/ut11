//
Функция РазмерКИЗ(Знач ЗначениеПоиска) Экспорт

	Если ЗначениеПоиска = Перечисления.РазмерыКиЗГИСМ.Размер25x160 Тогда
		Результат = "SMALL";
	ИначеЕсли ЗначениеПоиска = Перечисления.РазмерыКиЗГИСМ.Размер53x80 Тогда
		Результат = "BIG";

	ИначеЕсли ЗначениеПоиска = "SMALL" Тогда
		Результат = Перечисления.РазмерыКиЗГИСМ.Размер25x160;
	ИначеЕсли ЗначениеПоиска = "BIG" Тогда
		Результат = Перечисления.РазмерыКиЗГИСМ.Размер53x80;
	Иначе
		ВызватьИсключение
			СтрШаблон(
				НСтр("ru = 'Неизвестный размер КИЗ: %1'"),
				ЗначениеПоиска);
	КонецЕсли;

	Возврат Результат;
	
КонецФункции

Функция СтранаЭмиссииПоСерийномуНомеру(Знач КодСтраны) Экспорт
	
	Если КодСтраны = "1" Тогда
		Возврат Неопределено; // Республика Армения
	ИначеЕсли КодСтраны = "2" Тогда
		Возврат "112"; // Республика Беларусь
	ИначеЕсли КодСтраны = "3" Тогда
		Возврат "398"; // Республика Казахстан
	ИначеЕсли КодСтраны = "4" Тогда
		Возврат Неопределено; // Киргизская Республика
	ИначеЕсли КодСтраны = "5" Тогда
		Возврат Неопределено; // Российская Федерация
	Иначе
		Возврат Неопределено;
	КонецЕсли;
	
КонецФункции

