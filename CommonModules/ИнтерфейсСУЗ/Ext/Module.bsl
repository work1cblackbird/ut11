Процедура КонвертироватьТелоЗапросаФормат_V3(ТелоЗапроса, ВидПродукции, Поля)
	
	АтрибутыШапки     = Неопределено;
	ЕстьПоляДаты      = Поля.Свойство("ПоляДаты");
	ЕстьПоляУдаляемые = Поля.Свойство("ПоляУдаляемые");
	
	Для Каждого Элемент Из ТелоЗапроса Цикл
		Если Элемент.Ключ = Поля.ПолеАтрибутов Тогда
			Продолжить;
		КонецЕсли;
		Если Не Поля.ПоляШапки.Свойство(Элемент.Ключ) Тогда
			Если АтрибутыШапки = Неопределено Тогда
				ТелоЗапроса.Вставить(Поля.ПолеАтрибутов, Новый Структура);
				АтрибутыШапки = ТелоЗапроса[Поля.ПолеАтрибутов];
			КонецЕсли;
			Если ЕстьПоляДаты И Поля.ПоляДаты.Свойство(Элемент.Ключ) Тогда
				АтрибутыШапки.Вставить(Элемент.Ключ, ДатаВФормате_V3(Элемент.Значение));
			ИначеЕсли Не (ЕстьПоляУдаляемые И Поля.ПоляУдаляемые.Свойство(Элемент.Ключ)) Тогда
				АтрибутыШапки.Вставить(Элемент.Ключ, Элемент.Значение);
			КонецЕсли;
			ТелоЗапроса.Удалить(Элемент.Ключ);
		ИначеЕсли ТипЗнч(Поля.ПоляШапки[Элемент.Ключ]) = Тип("Структура") Тогда
			ПоляСтроки = Поля.ПоляШапки[Элемент.Ключ];
			Для Каждого Строка Из ТелоЗапроса[Элемент.Ключ] Цикл
				АтрибутыСтроки = Неопределено;
				Для Каждого ЭлементСтроки Из Строка Цикл
					Если ЭлементСтроки.Ключ = Поля.ПолеАтрибутов Тогда
						Продолжить;
					КонецЕсли;
					Если Не ПоляСтроки.Свойство(ЭлементСтроки.Ключ) Тогда
						Если АтрибутыСтроки = Неопределено Тогда
							Строка.Вставить(Поля.ПолеАтрибутов, Новый Структура);
							АтрибутыСтроки = Строка[Поля.ПолеАтрибутов];
						КонецЕсли;
						Строка[Поля.ПолеАтрибутов].Вставить(ЭлементСтроки.Ключ, ЭлементСтроки.Значение);
						Строка.Удалить(ЭлементСтроки.Ключ);
					КонецЕсли;
				КонецЦикла;
			КонецЦикла;
		КонецЕсли;
	КонецЦикла;
	
	ТелоЗапроса.Вставить(Поля.ПолеВидаПродукции, ВидПродукцииСУЗ_V3(ВидПродукции));
	
КонецПроцедуры

Функция ДатаВФормате_V3(Дата)
	
	Если СтрДлина(Дата) = 10 И Сред(Дата, 3, 1) = "." И Сред(Дата, 6, 1) = "." Тогда
		Возврат СтрШаблон("%1-%2-%3",
					Сред(Дата, 7, 4),
					Сред(Дата, 4, 2),
					Сред(Дата, 1, 2));
	ИначеЕсли СтрДлина(Дата) = 6 Тогда
		Возврат СтрШаблон("20%1-%2-%3",
					Сред(Дата, 1, 2),
					Сред(Дата, 3, 2),
					Сред(Дата, 5, 2));
	ИначеЕсли СтрДлина(Дата) = 10 И СтрНайти(Дата, ".") = 0 Тогда
		Возврат СтрШаблон("20%1-%2-%3T%4:%5",
					Сред(Дата, 1, 2),
					Сред(Дата, 3, 2),
					Сред(Дата, 5, 2),
					Сред(Дата, 7, 2),
					Сред(Дата, 9, 2));
	Иначе
		Возврат Дата;
	КонецЕсли;
	
КонецФункции

