	|	ДанныеДокумента.Назначение             КАК Назначение,
	|	ДанныеДокумента.Склад                  КАК Склад,
	|	ДанныеДокумента.Отправитель            КАК Отправитель,
	|	ДанныеДокумента.ХозяйственнаяОперация  КАК ХозяйственнаяОперация,
	|	ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка) КАК Серия,
	|	0                                      КАК КОформлениюНакладныхПоРаспоряжению,
	|	ДанныеДокумента.Количество             КАК КОформлениюПоступленийПоРаспоряжению,
	// Не используется, нужно для адаптированного текста.
	|	0                                      КАК КОформлениюПоступленийПоНакладным,
	|	0                                      КАК КОформлениюПоступленийПоОрдерам,
	|	0                                      КАК КОформлениюОрдеров
	|ИЗ
	|	ВТТоварыОформитьПоступлениеТоваровПоОдноходовке КАК ДанныеДокумента
	|ГДЕ
	|	ДанныеДокумента.Номенклатура.ТипНоменклатуры В
	|			(ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар),
	|			ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.МногооборотнаяТара))
	|	И ДанныеДокумента.Количество <> 0
	|
	|ОБЪЕДИНИТЬ ВСЕ
	|
	|ВЫБРАТЬ
	|	ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) КАК ВидДвижения,
	|	ДанныеДокумента.Ссылка                 КАК Регистратор,
	|	ДанныеДокумента.Дата                   КАК Период,
	|	&ДокументПоступленияОдноходовки        КАК ДокументПоступления,
	|	ДанныеДокумента.Номенклатура           КАК Номенклатура,
	|	ДанныеДокумента.Характеристика         КАК Характеристика,
	|	ДанныеДокумента.Назначение             КАК Назначение,
	|	ДанныеДокумента.Склад                  КАК Склад,
	|	ДанныеДокумента.Отправитель            КАК Отправитель,
	|	ДанныеДокумента.ХозяйственнаяОперация  КАК ХозяйственнаяОперация,
	|	ВЫБОР
	|		КОГДА ДанныеДокумента.СтатусУказанияСерий В (14, 18, 20) ТОГДА
	|			ДанныеДокумента.Серия
	|		ИНАЧЕ
	|			ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка)
	|	КОНЕЦ                                  КАК Серия,
	|	0                                      КАК КОформлениюНакладныхПоРаспоряжению,
	|	0                                      КАК КОформлениюПоступленийПоРаспоряжению,
	// Не используется, нужно для адаптированного текста.
	|	0                                      КАК КОформлениюПоступленийПоНакладным,
	|	ДанныеДокумента.Количество             КАК КОформлениюПоступленийПоОрдерам,
	|	0                                      КАК КОформлениюОрдеров
	|ИЗ
	|	ВТТоварыОформитьПоступлениеТоваровПоОдноходовке КАК ДанныеДокумента
	|ГДЕ
	|	&ИспользуетсяОрдернаяСхемаПриПоступлении
	|	И ДанныеДокумента.Номенклатура.ТипНоменклатуры В
	|			(ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар),
	|			ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.МногооборотнаяТара))
	|	И ДанныеДокумента.Количество <> 0";
	
	Если Не ТребуетсяУчитыватьСерииКОформлениюНакладныхПоРаспоряжению Тогда
		ШаблонТекстаЗапросаТоварыКПоступлению = СтрЗаменить(ШаблонТекстаЗапросаТоварыКПоступлению, "&СерияКОформлениюНакладныхПоРаспоряжению",
			"ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка)");
	КонецЕсли;
	Если ТребуетсяОтборПоОрдерностиСкладаНаОтражениеИзлишковНедостач Тогда
		ШаблонТекстаЗапросаТоварыНаСкладах = СтрЗаменить(ШаблонТекстаЗапросаТоварыНаСкладах, 
		"&ОтборПоОрдерностиИзлишковНедостач", "&ИспользуетсяОрдернаяСхемаПриОтраженииИзлишковНедостач");
	Иначе
		ШаблонТекстаЗапросаТоварыНаСкладах = СтрЗаменить(ШаблонТекстаЗапросаТоварыНаСкладах, 
		"&ОтборПоОрдерностиИзлишковНедостач", "ИСТИНА");
	КонецЕсли;
	
	ЗаполнитьПараметрыТекстаЗапроса(ШаблонТекстаЗапросаТоварыНаСкладах,		ПараметрыЗапроса);
	ЗаполнитьПараметрыТекстаЗапроса(ШаблонТекстаЗапросаТоварыКПоступлению,	ПараметрыЗапроса);
	
	ШаблонТекстаЗапросаТоварыНаСкладах		= СтрЗаменить(ШаблонТекстаЗапросаТоварыНаСкладах,
															"ДанныеДокумента",
															СинонимТаблицыДокумента);
	ШаблонТекстаЗапросаТоварыКПоступлению	= СтрЗаменить(ШаблонТекстаЗапросаТоварыКПоступлению,
															"ДанныеДокумента",
															СинонимТаблицыДокумента);
	
	ТекстыШаблоновОтражения = Новый Структура;
	
	Если ПроведениеДокументов.ТребуетсяТаблицаДляДвижений("ТоварыНаСкладах", Регистры) Тогда
		ТекстыШаблоновОтражения.Вставить("ТоварыНаСкладах", ШаблонТекстаЗапросаТоварыНаСкладах);
	КонецЕсли;
	
	Если ПроведениеДокументов.ТребуетсяТаблицаДляДвижений("ТоварыКПоступлению", Регистры) Тогда
		ТекстыШаблоновОтражения.Вставить("ТоварыКПоступлению", ШаблонТекстаЗапросаТоварыКПоступлению);
	КонецЕсли;
	
	ТекстыЗапросовДанныхДокумента = Новый Структура(СинонимТаблицыДокумента, ТекстЗапросаДокумента);
	
	ПроведениеДокументов.ДополнитьЗапросОтраженияДокумента(Запрос,
															ТекстыЗапроса,
															ТекстыШаблоновОтражения,
															ТекстыЗапросовДанныхДокумента,
															ТекстыЗапросовВременныхТаблиц);
	
КонецПроцедуры

Процедура ОформитьИзлишкиНедостачиНаСкладеВнутренний(Запрос, ТекстыЗапроса, Регистры,
		ТекстЗапросаДокумента, ТребуетсяОтборПоОрдерностиСкладаПриПоступлении = Ложь)

	ТекстыШаблоновОтражения = Новый Структура();
	ТекстыЗапросовДанныхДокумента = Новый Структура();
	ТекстыЗапросовВременныхТаблиц = Новый Структура();

	ТекстыЗапросовДанныхДокумента.Вставить("ДанныеДокумента", ТекстЗапросаДокумента);

	ШаблонТекстаЗапросаТоварыКОформлениюИзлишковНедостач = "ВЫБРАТЬ
		|	ВЫБОР
		|		КОГДА ДанныеДокумента.Количество > 0
		|			ТОГДА ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход) 
		|		ИНАЧЕ ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) 
		|	КОНЕЦ							КАК ВидДвижения,
		|	ДанныеДокумента.Ссылка			КАК Регистратор,
		|	ДанныеДокумента.Дата			КАК Период,
		|	ДанныеДокумента.Склад			КАК Склад,
		|	ДанныеДокумента.Номенклатура	КАК Номенклатура,
		|	ДанныеДокумента.Характеристика	КАК Характеристика,
		|	ВЫБОР
		|		КОГДА ЕСТЬNULL(ДанныеДокумента.Назначение.ДвиженияПоСкладскимРегистрам, ЛОЖЬ)
		|			ТОГДА ДанныеДокумента.Назначение
		|		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.Назначения.ПустаяСсылка)
		|	КОНЕЦ							КАК Назначение,
		|	ДанныеДокумента.Серия			КАК Серия,
		|	ВЫБОР
		|		КОГДА
		|			ДанныеДокумента.Количество < 0
		|			ТОГДА
		|				-ДанныеДокумента.Количество
		|			ИНАЧЕ
		|				ДанныеДокумента.Количество
		|	КОНЕЦ							КАК КОформлениюАктов
		|ИЗ
		|	#ДанныеДокумента КАК ДанныеДокумента
		|ГДЕ
		|	ДанныеДокумента.Склад.ИспользоватьОрдернуюСхемуПриОтраженииИзлишковНедостач
		|		И ДанныеДокумента.Дата >= ДанныеДокумента.Склад.ДатаНачалаОрдернойСхемыПриОтраженииИзлишковНедостач";

	ШаблонТекстаЗапросаТоварыНаСкладах = "";
	Если ТребуетсяОтборПоОрдерностиСкладаПриПоступлении Тогда
		ШаблонТекстаЗапросаТоварыНаСкладах = "ВЫБРАТЬ
			|	ВЫБОР
			|		КОГДА ДанныеДокумента.Количество > 0
			|			ТОГДА ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
			|		ИНАЧЕ ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
			|	КОНЕЦ				КАК ВидДвижения,
			|	ДанныеДокумента.Ссылка			КАК Регистратор,
			|	ДанныеДокумента.Дата			КАК Период,
			|	ДанныеДокумента.Номенклатура	КАК Номенклатура,
			|	ДанныеДокумента.Характеристика	КАК Характеристика,
			|	ВЫБОР
			|		КОГДА ЕСТЬNULL(ДанныеДокумента.Назначение.ДвиженияПоСкладскимРегистрам, ЛОЖЬ)
			|			ТОГДА ДанныеДокумента.Назначение
			|		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.Назначения.ПустаяСсылка)
			|	КОНЕЦ							КАК Назначение,
			|	ВЫБОР
			|		КОГДА ДанныеДокумента.СтатусУказанияСерий В (4, 6, 8, 10, 14)
			|			ТОГДА ДанныеДокумента.Серия
			|		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка)
			|	КОНЕЦ							КАК Серия,
			|	ДанныеДокумента.Склад			КАК Склад,
			|	ЗНАЧЕНИЕ(Справочник.СкладскиеПомещения.ПустаяСсылка) КАК Помещение,
			|	ВЫБОР
			|		КОГДА
			|			ДанныеДокумента.Количество < 0
			|			ТОГДА 
			|				-ДанныеДокумента.Количество
			|			ИНАЧЕ
			|				ДанныеДокумента.Количество
			|	КОНЕЦ							КАК ВНаличии,
			|	0								КАК КОтгрузке,
			|	ЛОЖЬ							КАК КонтролироватьОстатки
			|ИЗ
			|	#ДанныеДокумента КАК ДанныеДокумента
			|ГДЕ
			|	(
			|	((ДанныеДокумента.Склад.ИспользоватьОрдернуюСхемуПриПоступлении
			|		И ДанныеДокумента.Дата >= ДанныеДокумента.Склад.ДатаНачалаОрдернойСхемыПриПоступлении)
			|		И ДанныеДокумента.РазрешеноОтражатьОстатокПоОрдерномуСкладу)
			|	ИЛИ
			|	( НЕ (ДанныеДокумента.Склад.ИспользоватьОрдернуюСхемуПриПоступлении
			|		И ДанныеДокумента.Дата >= ДанныеДокумента.Склад.ДатаНачалаОрдернойСхемыПриПоступлении)
			|		И ДанныеДокумента.РазрешеноОтражатьОстатокПоНеОрдерномуСкладу)
			|	)
			|	И НЕ (ДанныеДокумента.Склад.ИспользоватьОрдернуюСхемуПриОтраженииИзлишковНедостач
			|		И ДанныеДокумента.Дата >= ДанныеДокумента.Склад.ДатаНачалаОрдернойСхемыПриОтраженииИзлишковНедостач)
			|	И ДанныеДокумента.Номенклатура.ТипНоменклатуры В
			|			(ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар),
			|			 ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.МногооборотнаяТара))";
	Иначе
		ШаблонТекстаЗапросаТоварыНаСкладах = "ВЫБРАТЬ
		|	ВЫБОР
		|		КОГДА ДанныеДокумента.Количество > 0
		|			ТОГДА ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
		|		ИНАЧЕ ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
		|	КОНЕЦ				КАК ВидДвижения,
		|	ДанныеДокумента.Ссылка			КАК Регистратор,
		|	ДанныеДокумента.Дата			КАК Период,
		|	ДанныеДокумента.Номенклатура	КАК Номенклатура,
		|	ДанныеДокумента.Характеристика	КАК Характеристика,
		|	ВЫБОР
		|		КОГДА ЕСТЬNULL(ДанныеДокумента.Назначение.ДвиженияПоСкладскимРегистрам, ЛОЖЬ)
		|			ТОГДА ДанныеДокумента.Назначение
		|		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.Назначения.ПустаяСсылка)
		|	КОНЕЦ							КАК Назначение,
		|	ВЫБОР
		|		КОГДА ДанныеДокумента.СтатусУказанияСерий В (4, 6, 8, 10, 14)
		|			ТОГДА ДанныеДокумента.Серия
		|		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка)
		|	КОНЕЦ							КАК Серия,
		|	ДанныеДокумента.Склад			КАК Склад,
		|	ЗНАЧЕНИЕ(Справочник.СкладскиеПомещения.ПустаяСсылка) КАК Помещение,
		|	ВЫБОР
		|		КОГДА
		|			ДанныеДокумента.Количество < 0
		|			ТОГДА 
		|				-ДанныеДокумента.Количество
		|			ИНАЧЕ
		|				ДанныеДокумента.Количество
		|	КОНЕЦ							КАК ВНаличии,
		|	0								КАК КОтгрузке,
		|	ЛОЖЬ							КАК КонтролироватьОстатки
		|ИЗ
		|	#ДанныеДокумента КАК ДанныеДокумента
		|ГДЕ
		|	НЕ (ДанныеДокумента.Склад.ИспользоватьОрдернуюСхемуПриОтраженииИзлишковНедостач
		|		И ДанныеДокумента.Дата >= ДанныеДокумента.Склад.ДатаНачалаОрдернойСхемыПриОтраженииИзлишковНедостач)
		|	И ДанныеДокумента.Номенклатура.ТипНоменклатуры В
		|			(ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар),
		|			 ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.МногооборотнаяТара))";
	КонецЕсли;
	
	Если ПроведениеДокументов.ТребуетсяТаблицаДляДвижений("ТоварыКОформлениюИзлишковНедостач", Регистры) Тогда
		ТекстыШаблоновОтражения.Вставить("ТоварыКОформлениюИзлишковНедостач", ШаблонТекстаЗапросаТоварыКОформлениюИзлишковНедостач);
	КонецЕсли;
	
	Если ПроведениеДокументов.ТребуетсяТаблицаДляДвижений("ТоварыНаСкладах", Регистры) Тогда
		ТекстыШаблоновОтражения.Вставить("ТоварыНаСкладах", ШаблонТекстаЗапросаТоварыНаСкладах);
	КонецЕсли;
	
	ПроведениеДокументов.ДополнитьЗапросОтраженияДокумента(Запрос, ТекстыЗапроса, ТекстыШаблоновОтражения, ТекстыЗапросовДанныхДокумента, ТекстыЗапросовВременныхТаблиц);
	
КонецПроцедуры

Процедура ОформитьОтгрузкуТоваровВнутренний(Запрос,
									ТекстыЗапроса,
									Регистры,
									ТекстЗапросаДанныхДокумента,
									ПараметрыМодульногоПроведения)
	
	ОбъектМетаданных = ПараметрыМодульногоПроведения.ОбъектМетаданных;
	ИмяТЧСерии = ПараметрыМодульногоПроведения.ИмяТЧСерии;
	СинонимТЧДокумента = ПараметрыМодульногоПроведения.СинонимТаблицыДокумента;
	ТребуетсяОтборПоОрдерностиСкладаПриПоступлении = ПараметрыМодульногоПроведения.ТребуетсяОтборПоОрдерностиСкладаПриПоступлении;
	ТребуетсяОтборПоОрдерностиСкладаНаОтражениеИзлишковНедостач = ПараметрыМодульногоПроведения.ТребуетсяОтборПоОрдерностиСкладаНаОтражениеИзлишковНедостач;
	
	Если ТребуетсяОтборПоОрдерностиСкладаПриПоступлении
		И ТребуетсяОтборПоОрдерностиСкладаНаОтражениеИзлишковНедостач Тогда
	ШаблонТекстаЗапросаТоварыКОтгрузке =
		// Оформление отгрузки с неордерного склада по накладной на основании распоряжения (расход по "КОтгрузке"
		// и "КОформлению").
		"ВЫБРАТЬ
		|	&ВидДвиженияРасход				КАК ВидДвижения,
		|	ДанныеДокумента.Ссылка			КАК Регистратор,
		|	ДанныеДокумента.Период			КАК Период,
		|	ДанныеДокумента.Склад			КАК Склад,
		|	ДанныеДокумента.Получатель		КАК Получатель,
		|	&ДокументОтгрузки				КАК ДокументОтгрузки,
		|	ДанныеДокумента.Номенклатура	КАК Номенклатура,
		|	ДанныеДокумента.Характеристика	КАК Характеристика,
		|	&Назначение						КАК Назначение,
