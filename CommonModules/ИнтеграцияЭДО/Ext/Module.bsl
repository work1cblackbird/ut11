Функция УКД2020_ИнформацияПокупателя_ДанныеУчета(Знач Основания, Знач Параметры)
	
	Данные = УКД2020_ИнформацияПокупателя_НовыеДанные();
	
	СтруктураОтвета = Параметры.ДанныеРучногоФормированияТитула;
	Если ЗначениеЗаполнено(СтруктураОтвета) Тогда
		УКД2020_ИнформацияПокупателя_ЗаполнитьДанныеРучногоФормирования(Данные, СтруктураОтвета);
		Результат = НовыйРезультатФормированияДанныхОбъектаУчета();
		Результат.Данные = Данные;
		Возврат Результат;
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(Основания) И Не ПолучитьФункциональнуюОпцию("ЗапретитьФормированиеОтветногоТитулаБезДокументовУчета") Тогда
		Результат = НовыйРезультатФормированияДанныхОбъектаУчета();
		Результат.ОшибкаФормированияВПрикладнойЧасти = Истина;	
		Возврат Результат;	
	КонецЕсли;
	
	Отказ = Ложь;
	Попытка
		ОбменСКонтрагентамиПереопределяемый.ЗаполнитьДанныеУКД2020_ИнформацияПокупателя(
			Основания, Данные, Отказ);
	Исключение
		Если Не ПолучитьФункциональнуюОпцию("ЗапретитьФормированиеОтветногоТитулаБезДокументовУчета") Тогда
			Результат = НовыйРезультатФормированияДанныхОбъектаУчета();
			Результат.ОшибкаФормированияВПрикладнойЧасти = Истина;
			Возврат Результат;
		КонецЕсли;
		Возврат НовыйРезультатФормированияДанныхОбъектаУчета();	 
	КонецПопытки;
	
	Если Отказ Тогда
		Если Не ПолучитьФункциональнуюОпцию("ЗапретитьФормированиеОтветногоТитулаБезДокументовУчета") Тогда
			Результат = НовыйРезультатФормированияДанныхОбъектаУчета();
			Результат.ОшибкаФормированияВПрикладнойЧасти = Истина;
			Возврат Результат;
		Иначе
			Возврат НовыйРезультатФормированияДанныхОбъектаУчета();	
		КонецЕсли;
	КонецЕсли;
		
	Результат = НовыйРезультатФормированияДанныхОбъектаУчета();
	Результат.Данные = Данные;
	Результат.Формат = "УКД_ИнформацияПокупателя_2020";
	
	Возврат Результат;
		
КонецФункции

