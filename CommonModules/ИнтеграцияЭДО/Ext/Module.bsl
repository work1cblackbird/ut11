// Заполняет значения ставки НДС в дереве значений электронного документа.
// 
// Параметры:
// 	Данные - ДеревоЗначений
//
Процедура УПД_5_02_ИнформацияПродавца_ЗаполнитьНалоговуюСтавкуИзСоответствия(Данные)
	
	Путь = "ТаблицаСчетаФактуры.СведенияОбОтгруженныхПозициях";
	
	СведенияОТоварах = ДеревоЭлектронногоДокументаБЭД.СтрокаДерева(Данные, Путь, Истина);
	
	Для Каждого СведенияОТоваре Из СведенияОТоварах.Строки Цикл
		
		ЗаполнитьНалоговуюСтавкуИзСоответствия(СведенияОТоваре, Путь + ".НомерСтроки.НалоговаяСтавка");
		
		ЗаполнитьНалоговуюСтавкуИзСоответствия(СведенияОТоваре, Путь + ".НомерСтроки.Сопоставление.СтавкаНДС");
		
	КонецЦикла;
	
КонецПроцедуры

// Добавляет дополнительные данные в дерево значений электронного документа.
// 
// Параметры:
// 	Данные - ДеревоЗначений
//
Процедура УПД_5_02_ИнформацияПродавца_ДобавитьДополнительныеДанные(Данные)
	
	ПоддерживаемыеФорматы = ФорматыЭДО_ФНС.ПоддерживаемыеФорматы();
	Формат = ПоддерживаемыеФорматы.УПД_5_02.ИнформацияПродавца;
	
	Путь = "ИнформационноеПолеФХЖ.ТекстоваяИнформация";
	ИнформационноеПолеФХЖ1 = "СведенияОСчетеФактуре." + Путь;
	ИнформационноеПолеФХЖ2 = "ТаблицаСчетаФактуры.СведенияОбОтгруженныхПозициях.НомерСтроки.ИнформационноеПолеФХЖ";
	ИнформационноеПолеФХЖ3 = "СведенияОПередаче." + Путь;
	
	ТекстоваяИнформация = Новый ТаблицаЗначений;
	ТекстоваяИнформация.Колонки.Добавить("Идентификатор", Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(50)));
	ТекстоваяИнформация.Колонки.Добавить("Значение", Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(2000)));
	
	ДобавитьСведенияОВыбытииМаркированныхТоваров(Данные, ТекстоваяИнформация, Формат);
			
	ДокументыОснования = Данные.Строки.Найти("ДокументыОснованияСчетаФактуры", "ПолныйПуть").Значение;
	Если ЗначениеЗаполнено(ДокументыОснования) Тогда
		Если ТипЗнч(ДокументыОснования) <> Тип("Массив") Тогда
			ДокументыОснования = ОбщегоНазначенияКлиентСервер.ЗначениеВМассиве(ДокументыОснования);
		КонецЕсли;
		Для Каждого Основание Из ДокументыОснования Цикл
			ИдентификаторОснования = Строка(Основание.УникальныйИдентификатор());
			НоваяСтрока = ТекстоваяИнформация.Добавить();
			НоваяСтрока.Идентификатор = "ИдентификаторДокументаОснования";
			НоваяСтрока.Значение = ИдентификаторОснования;
			ДеревоЭлектронногоДокументаБЭД.ДобавитьЗаписьВТаблицуДерева(Данные, НоваяСтрока, ИнформационноеПолеФХЖ1);
		КонецЦикла;
	КонецЕсли;
	
	ДокументыОснования = Данные.Строки.Найти("ДокументыОснованияДокументаОтгрузки", "ПолныйПуть").Значение;
