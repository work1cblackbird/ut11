	НовоеПравило.ЕстьОбработчикПослеОбработки = Ложь;
		
	УстановитьОтборы(НовоеПравило, 
		Справочник_Склады_ОтборПоСвойствамПланаОбмена(),
		ОтборПоСвойствамОбъекта_7());
