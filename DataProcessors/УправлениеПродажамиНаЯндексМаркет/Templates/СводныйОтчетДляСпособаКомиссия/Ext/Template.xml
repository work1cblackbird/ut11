<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>ТипОперации</dataPath>
			<field>ТипОперации</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Тип операции</v8:content>
				</v8:item>
			</title>
			<valueType>
				<v8:Type>xs:decimal</v8:Type>
				<v8:NumberQualifiers>
					<v8:Digits>2</v8:Digits>
					<v8:FractionDigits>0</v8:FractionDigits>
					<v8:AllowedSign>Any</v8:AllowedSign>
				</v8:NumberQualifiers>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ИсточникКоличество</dataPath>
			<field>ИсточникКоличество</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Количество</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ИсточникСумма</dataPath>
			<field>ИсточникСумма</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Стоимость с учетом скидок</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ИсточникСуммаСкидки</dataPath>
			<field>ИсточникСуммаСкидки</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сумма скидки</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПриемникКоличество</dataPath>
			<field>ПриемникКоличество</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Количество</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПриемникСумма</dataPath>
			<field>ПриемникСумма</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Стоимость с учетом скидок</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПриемникСуммаСкидки</dataPath>
			<field>ПриемникСуммаСкидки</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сумма скидки</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СтавкаНДС</dataPath>
			<field>СтавкаНДС</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Ставка НДС</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ВЫРАЗИТЬ(СопоставленныеДанные.ТипОперации КАК ЧИСЛО(2, 0)) КАК ТипОперации,
	ВЫРАЗИТЬ(СопоставленныеДанные.НомерЗаказа КАК СТРОКА(50)) КАК НомерЗаказа,
	ВЫРАЗИТЬ(СопоставленныеДанные.ДатаОформленияЗаказа КАК ДАТА) КАК ДатаОформленияЗаказа,
	ВЫРАЗИТЬ(СопоставленныеДанные.ДатаПередачиВДоставку КАК ДАТА) КАК ДатаПередачиВДоставку,
	ВЫРАЗИТЬ(СопоставленныеДанные.ДатаДоставки КАК ДАТА) КАК ДатаДоставки,
	ВЫРАЗИТЬ(СопоставленныеДанные.ДатаНевыкупаВозврата КАК ДАТА) КАК ДатаНевыкупаВозврата,
	ВЫРАЗИТЬ(СопоставленныеДанные.КоличествоПередано КАК ЧИСЛО(15, 3)) КАК КоличествоПередано,
	ВЫРАЗИТЬ(СопоставленныеДанные.КоличествоДоставлено КАК ЧИСЛО(15, 3)) КАК КоличествоДоставлено,
	ВЫРАЗИТЬ(СопоставленныеДанные.КоличествоВозврат КАК ЧИСЛО(15, 3)) КАК КоличествоВозврат,
	ВЫРАЗИТЬ(СопоставленныеДанные.СтавкаНДС КАК СТРОКА(25)) КАК СтавкаНДС,
	ВЫРАЗИТЬ(СопоставленныеДанные.Сумма КАК ЧИСЛО(20, 2)) КАК Сумма,
	ВЫРАЗИТЬ(СопоставленныеДанные.СуммаСкидки КАК ЧИСЛО(20, 2)) КАК СуммаСкидки
ПОМЕСТИТЬ ВТ_СопоставленныеДанные
ИЗ
	&amp;СопоставленныеДанные КАК СопоставленныеДанные
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СопоставленныеДанные.ТипОперации КАК ТипОперации,
	ВТ_СопоставленныеДанные.НомерЗаказа КАК НомерЗаказа,
	ВТ_СопоставленныеДанные.ДатаОформленияЗаказа КАК ДатаОформленияЗаказа,
	СУММА(ВТ_СопоставленныеДанные.СуммаСкидки) КАК СуммаСкидки
ПОМЕСТИТЬ ВТ_СопоставленныеДанныеИтоги
ИЗ
	ВТ_СопоставленныеДанные КАК ВТ_СопоставленныеДанные

СГРУППИРОВАТЬ ПО
	ВТ_СопоставленныеДанные.ТипОперации,
	ВТ_СопоставленныеДанные.НомерЗаказа,
	ВТ_СопоставленныеДанные.ДатаОформленияЗаказа
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДокументОтчетКомиссионераТовары.НомерДокументаОтгрузки КАК НомерДокументаОтгрузки,
	НАЧАЛОПЕРИОДА(ДокументОтчетКомиссионераТовары.ДатаДокументаОтгрузки, ДЕНЬ) КАК ДатаДокументаОтгрузки,
	ЕСТЬNULL(ДокументОтчетКомиссионераТовары.СтавкаНДС.ПеречислениеСтавкаНДС, ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС)) КАК СтавкаНДС,
	СУММА(ДокументОтчетКомиссионераТовары.КоличествоУпаковок) КАК КоличествоУпаковок,
	СУММА(ДокументОтчетКомиссионераТовары.СуммаСНДС) КАК СуммаСНДС
ПОМЕСТИТЬ ВТ_ОтчетыКомиссионераДоставленные
ИЗ
	Документ.ОтчетКомиссионера.Товары КАК ДокументОтчетКомиссионераТовары
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ОтчетКомиссионера КАК ДокументОтчетКомиссионера
		ПО (ДокументОтчетКомиссионера.Ссылка = ДокументОтчетКомиссионераТовары.Ссылка)
ГДЕ
	ДокументОтчетКомиссионераТовары.КоличествоУпаковок &gt; 0
	И ДокументОтчетКомиссионера.Ссылка В(&amp;ЗагруженныеОбъекты)
	И ДокументОтчетКомиссионера.Организация = &amp;Организация
	И ДокументОтчетКомиссионера.Партнер = &amp;Партнер
	И ДокументОтчетКомиссионера.Контрагент = &amp;Контрагент
	И НЕ ДокументОтчетКомиссионера.ПометкаУдаления

СГРУППИРОВАТЬ ПО
	ДокументОтчетКомиссионераТовары.НомерДокументаОтгрузки,
	НАЧАЛОПЕРИОДА(ДокументОтчетКомиссионераТовары.ДатаДокументаОтгрузки, ДЕНЬ),
	ЕСТЬNULL(ДокументОтчетКомиссионераТовары.СтавкаНДС.ПеречислениеСтавкаНДС, ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС))
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДокументОтчетКомиссионераТовары.НомерДокументаОтгрузки КАК НомерДокументаОтгрузки,
	НАЧАЛОПЕРИОДА(ДокументОтчетКомиссионераТовары.ДатаДокументаОтгрузки, ДЕНЬ) КАК ДатаДокументаОтгрузки,
	ЕСТЬNULL(ДокументОтчетКомиссионераТовары.СтавкаНДС.ПеречислениеСтавкаНДС, ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС)) КАК СтавкаНДС,
	СУММА(ДокументОтчетКомиссионераТовары.КоличествоУпаковок) КАК КоличествоУпаковок,
	СУММА(ДокументОтчетКомиссионераТовары.СуммаСНДС) КАК СуммаСНДС
ПОМЕСТИТЬ ВТ_ОтчетыКомиссионераВозвращенные
ИЗ
	Документ.ОтчетКомиссионера.Товары КАК ДокументОтчетКомиссионераТовары
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ОтчетКомиссионера КАК ДокументОтчетКомиссионера
		ПО (ДокументОтчетКомиссионера.Ссылка = ДокументОтчетКомиссионераТовары.Ссылка)
ГДЕ
	ДокументОтчетКомиссионераТовары.КоличествоУпаковок &lt; 0
	И ДокументОтчетКомиссионера.Ссылка В(&amp;ЗагруженныеОбъекты)
	И ДокументОтчетКомиссионера.Организация = &amp;Организация
	И ДокументОтчетКомиссионера.Партнер = &amp;Партнер
	И ДокументОтчетКомиссионера.Контрагент = &amp;Контрагент
	И НЕ ДокументОтчетКомиссионера.ПометкаУдаления

СГРУППИРОВАТЬ ПО
	ДокументОтчетКомиссионераТовары.НомерДокументаОтгрузки,
	НАЧАЛОПЕРИОДА(ДокументОтчетКомиссионераТовары.ДатаДокументаОтгрузки, ДЕНЬ),
	ЕСТЬNULL(ДокументОтчетКомиссионераТовары.СтавкаНДС.ПеречислениеСтавкаНДС, ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС))
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	2 КАК ТипОперации,
	ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС) КАК СтавкаНДС,
	0 КАК ИсточникКоличество,
	0 КАК ИсточникСумма,
	0 КАК ИсточникСуммаСкидки,
	0 КАК ПриемникКоличество,
	0 КАК ПриемникСумма,
	0 КАК ПриемникСуммаСкидки

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	3,
	ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС),
	0,
	0,
	0,
	0,
	0,
	0

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	4,
	ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС),
	0,
	0,
	0,
	0,
	0,
	0

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_СопоставленныеДанные.ТипОперации,
	ВЫБОР
		КОГДА ВТ_СопоставленныеДанные.СтавкаНДС = &amp;СтавкаНДСБезНДС
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС)
		КОГДА ВТ_СопоставленныеДанные.СтавкаНДС ПОДОБНО &amp;СтавкаНДС10
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
		КОГДА ВТ_СопоставленныеДанные.СтавкаНДС ПОДОБНО &amp;СтавкаНДС18
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
		КОГДА ВТ_СопоставленныеДанные.СтавкаНДС ПОДОБНО &amp;СтавкаНДС20
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.ПустаяСсылка)
	КОНЕЦ,
	ВЫБОР
		КОГДА ВТ_СопоставленныеДанные.ТипОперации = 1
			ТОГДА ВТ_СопоставленныеДанные.КоличествоПередано
		КОГДА ВТ_СопоставленныеДанные.ТипОперации = 2
			ТОГДА ВТ_СопоставленныеДанные.КоличествоДоставлено
		КОГДА ВТ_СопоставленныеДанные.ТипОперации = 3
				ИЛИ ВТ_СопоставленныеДанные.ТипОперации = 4
			ТОГДА ВТ_СопоставленныеДанные.КоличествоВозврат
		ИНАЧЕ 0
	КОНЕЦ,
	ВЫБОР
		КОГДА ВТ_СопоставленныеДанные.ТипОперации = 1
				ИЛИ ВТ_СопоставленныеДанные.ТипОперации = 2
			ТОГДА 1
		ИНАЧЕ -1
	КОНЕЦ * ВТ_СопоставленныеДанные.Сумма,
	ВЫБОР
		КОГДА ВТ_СопоставленныеДанные.ТипОперации = 1
				ИЛИ ВТ_СопоставленныеДанные.ТипОперации = 2
			ТОГДА 1
		ИНАЧЕ -1
	КОНЕЦ * ВТ_СопоставленныеДанные.СуммаСкидки,
	0,
	0,
	0
ИЗ
	ВТ_СопоставленныеДанные КАК ВТ_СопоставленныеДанные
ГДЕ
	ВТ_СопоставленныеДанные.ТипОперации &lt;&gt; 1

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	2,
	ВТ_ОтчетыКомиссионераДоставленные.СтавкаНДС,
	0,
	0,
	0,
	ВТ_ОтчетыКомиссионераДоставленные.КоличествоУпаковок,
	ВТ_ОтчетыКомиссионераДоставленные.СуммаСНДС,
	ЕСТЬNULL(ВТ_СопоставленныеДанныеИтоги.СуммаСкидки, 0)
ИЗ
	ВТ_ОтчетыКомиссионераДоставленные КАК ВТ_ОтчетыКомиссионераДоставленные
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СопоставленныеДанныеИтоги КАК ВТ_СопоставленныеДанныеИтоги
		ПО ВТ_ОтчетыКомиссионераДоставленные.НомерДокументаОтгрузки = ВТ_СопоставленныеДанныеИтоги.НомерЗаказа
			И ВТ_ОтчетыКомиссионераДоставленные.ДатаДокументаОтгрузки = ВТ_СопоставленныеДанныеИтоги.ДатаОформленияЗаказа
			И (ВТ_СопоставленныеДанныеИтоги.ТипОперации = 2)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ЕСТЬNULL(ВТ_СопоставленныеДанныеИтоги.ТипОперации, 4),
	ВТ_ОтчетыКомиссионераВозвращенные.СтавкаНДС,
	0,
	0,
	0,
	-ВТ_ОтчетыКомиссионераВозвращенные.КоличествоУпаковок,
	-ВТ_ОтчетыКомиссионераВозвращенные.СуммаСНДС,
	-ЕСТЬNULL(ВТ_СопоставленныеДанныеИтоги.СуммаСкидки, 0)
ИЗ
	ВТ_ОтчетыКомиссионераВозвращенные КАК ВТ_ОтчетыКомиссионераВозвращенные
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СопоставленныеДанныеИтоги КАК ВТ_СопоставленныеДанныеИтоги
		ПО ВТ_ОтчетыКомиссионераВозвращенные.НомерДокументаОтгрузки = ВТ_СопоставленныеДанныеИтоги.НомерЗаказа
			И ВТ_ОтчетыКомиссионераВозвращенные.ДатаДокументаОтгрузки = ВТ_СопоставленныеДанныеИтоги.ДатаОформленияЗаказа
			И (ВТ_СопоставленныеДанныеИтоги.ТипОперации = 3
				ИЛИ ВТ_СопоставленныеДанныеИтоги.ТипОперации = 4)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	2,
	ВТ_ОтчетыКомиссионераВозвращенные.СтавкаНДС,
	0,
	0,
	0,
	ВТ_ОтчетыКомиссионераВозвращенные.КоличествоУпаковок,
	ВТ_ОтчетыКомиссионераВозвращенные.СуммаСНДС,
	0
ИЗ
	ВТ_ОтчетыКомиссионераВозвращенные КАК ВТ_ОтчетыКомиссионераВозвращенные
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_СопоставленныеДанныеИтоги КАК ВТ_СопоставленныеДанныеИтоги
		ПО ВТ_ОтчетыКомиссионераВозвращенные.НомерДокументаОтгрузки = ВТ_СопоставленныеДанныеИтоги.НомерЗаказа
			И ВТ_ОтчетыКомиссионераВозвращенные.ДатаДокументаОтгрузки = ВТ_СопоставленныеДанныеИтоги.ДатаОформленияЗаказа
			И (ВТ_СопоставленныеДанныеИтоги.ТипОперации = 3)</query>
	</dataSet>
	<calculatedField>
		<dataPath>ИсточникСуммаБезСкидок</dataPath>
		<expression>ИсточникСумма + ИсточникСуммаСкидки</expression>
