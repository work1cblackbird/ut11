			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Таблица.Ссылка,
	Таблица.ПометкаУдаления,
	Таблица.Номер,
	Таблица.Дата,
	Таблица.Проведен,
	
	Таблица.МестоХранения     КАК МестоХранения,
	Таблица.ОрганизацияСАТУРН КАК ОрганизацияСАТУРН,
	Таблица.Ответственный     КАК Ответственный,
	Таблица.ДокументОснование КАК ДокументОснование,
	Таблица.ДатаПроизводства  КАК ДатаПроизводства,
	
	СтатусыДокументов.Статус              КАК Статус,
	СтатусыДокументов.ДальнейшееДействие1 КАК ДальнейшееДействие1,
	СтатусыДокументов.ДальнейшееДействие2 КАК ДальнейшееДействие2,
	СтатусыДокументов.ДальнейшееДействие3 КАК ДальнейшееДействие3
ИЗ
	Документ.ПроизводственнаяОперацияСАТУРН КАК Таблица
	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СтатусыДокументовСАТУРН КАК СтатусыДокументов
		ПО (СтатусыДокументов.Документ = Таблица.Ссылка)
//%СоединениеСОснованиями%
ГДЕ
	(Таблица.ДокументОснование В (&amp;ПустойДокументОснование)
//%ОтборПоДоступнымОснованиям%
	)
</QueryText>
