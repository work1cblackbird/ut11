					<dcssch:dataPath>ДатаВедомостиНаВыплатуЗарплаты</dcssch:dataPath>
					<dcssch:field>ДатаВедомостиНаВыплатуЗарплаты</dcssch:field>
				</Field>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>ДополнительныеРеквизиты</dcssch:dataPath>
					<dcssch:field>ДополнительныеРеквизиты</dcssch:field>
				</Field>
				<Field xsi:type="dcssch:DataSetFieldField">
