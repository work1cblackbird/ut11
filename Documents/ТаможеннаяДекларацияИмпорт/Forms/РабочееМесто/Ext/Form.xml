			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>КОформлению.ДокументПоступления</Field>
				<Field>КОформлению.КонтрагентПоставщика</Field>
				<Field>КОформлению.Организация</Field>
