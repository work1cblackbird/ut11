&НаСервере
Процедура НастроитьФорму()
	
	МассивЭлементов = Новый Массив;
	МассивЭлементов.Добавить("ТоварыЗаполнитьНоменклатуруПоНоменклатуреПартнера");
	МассивЭлементов.Добавить("ГруппаТоварыЦены");
	МассивЭлементов.Добавить("ТоварыТипМестаХранения");
	
	ВидимостьЭлементов = Истина;
	
	Если Объект.ХозяйственнаяОперация = Перечисления.ХозяйственныеОперации.ВыкупПринятыхНаХранениеТоваров Тогда
		
		АвтоЗаголовок = Истина;
		Заголовок     = "";
		
		Элементы.Партнер.Заголовок = НСтр("ru = 'Поставщик'");
		
		Элементы.ТоварыПодобратьТоварыПринятыеНаОтветственноеХранение.Заголовок =
			НСтр("ru = 'Подобрать товары, принятые на ответственное хранение'");
		
		
		ВидимостьЭлементов = Истина;
		
	ИначеЕсли Объект.ХозяйственнаяОперация = Перечисления.ХозяйственныеОперации.ПоставкаПодПринципала Тогда
		
		АвтоЗаголовок = Истина;
		Заголовок     = "";
		
		Элементы.Партнер.Заголовок = НСтр("ru = 'Принципал'");
		
		Элементы.ТоварыПодобратьТоварыПринятыеНаОтветственноеХранение.Заголовок =
			НСтр("ru = 'Подобрать товары, принятые на ответственное хранение'");
		
		
		ВидимостьЭлементов = Ложь;
		
	КонецЕсли;
	
	ОбщегоНазначенияУТКлиентСервер.УстановитьСвойствоЭлементовФормы(
		Элементы, МассивЭлементов, "Видимость", ВидимостьЭлементов);
	
	УстановитьВидимостьСоглашений(Элементы, Объект);
	
	НастроитьПараметрыВыбораЭлементов();
	
КонецПроцедуры

&НаСервере
Процедура НастроитьПараметрыВыбораЭлементов()
	
	// Партнер
	ПараметрыВыбораПартнер       = Новый Массив;
	
	// Группа фин. учета расчетов
	ПараметрыВыбораГФУ           = Новый Массив;
	
	//Договор
	ПараметрыВыбораДоговор       = Новый Массив;
	ПараметрыВыбораДоговор.Добавить(Новый ПараметрВыбора("Отбор.ПометкаУдаления", Ложь));
	ПараметрыВыбораДоговор.Добавить(Новый ПараметрВыбора("Отбор.ХозяйственнаяОперация", ХозяйственнаяОперацияДоговора));
	
	ПараметрыВыбораДоговор.Добавить(
		Новый ПараметрВыбора("Отбор.Статус", Перечисления.СтатусыДоговоровКонтрагентов.Действует));
	
	СвязиПараметровВыбораДоговор = Новый Массив;
	СвязиПараметровВыбораДоговор.Добавить(
		Новый СвязьПараметраВыбора(
				"Отбор.ВалютаВзаиморасчетов", "Объект.ВалютаВзаиморасчетов", РежимИзмененияСвязанногоЗначения.Очищать));
		
	СвязиПараметровВыбораДоговор.Добавить(
		Новый СвязьПараметраВыбора(
				"Отбор.Контрагент", "Объект.Контрагент", РежимИзмененияСвязанногоЗначения.Очищать));
		
	СвязиПараметровВыбораДоговор.Добавить(
		Новый СвязьПараметраВыбора(
				"Отбор.Организация", "Объект.Организация", РежимИзмененияСвязанногоЗначения.Очищать));
		
	СвязиПараметровВыбораДоговор.Добавить(
		Новый СвязьПараметраВыбора(
				"Партнер", "Объект.Партнер", РежимИзмененияСвязанногоЗначения.Очищать));
	
	Если Объект.ХозяйственнаяОперация = Перечисления.ХозяйственныеОперации.ВыкупПринятыхНаХранениеТоваров Тогда
	
		// Партнер
		ПараметрыВыбораПартнер.Добавить(Новый ПараметрВыбора("Отбор.Поставщик", Истина));
		
		// Группа фин. учета расчетов
		ПараметрыВыбораГФУ.Добавить(Новый ПараметрВыбора("Отбор.РасчетыСПоставщиками", Истина));
		
		//Договор
		ПараметрыВыбораДоговор.Добавить(
			Новый ПараметрВыбора("Отбор.ТипДоговора", Перечисления.ТипыДоговоров.СПоклажедателем));
		
		СвязиПараметровВыбораДоговор.Добавить(
			Новый СвязьПараметраВыбора(
					"Соглашение", "Объект.Соглашение", РежимИзмененияСвязанногоЗначения.НеИзменять));
	
	ИначеЕсли Объект.ХозяйственнаяОперация = Перечисления.ХозяйственныеОперации.ПоставкаПодПринципала Тогда
	
		// Партнер
		ПараметрыВыбораПартнер.Добавить(Новый ПараметрВыбора("Отбор.Клиент", Истина));
		
		// Группа фин. учета расчетов
		ПараметрыВыбораГФУ.Добавить(Новый ПараметрВыбора("Отбор.РасчетыСПоставщиками", Истина));
		
		//Договор
		ПараметрыВыбораДоговор.Добавить(
			Новый ПараметрВыбора("Отбор.ТипДоговора", Перечисления.ТипыДоговоров.СКомитентомНаЗакупку));
		
		СвязиПараметровВыбораДоговор.Добавить(
			Новый СвязьПараметраВыбора(
					"Соглашение", "Объект.Соглашение", РежимИзмененияСвязанногоЗначения.НеИзменять));
	
	КонецЕсли;
	
	Элементы.Партнер.ПараметрыВыбора                = Новый ФиксированныйМассив(ПараметрыВыбораПартнер);
	
	Элементы.Договор.ПараметрыВыбора                = Новый ФиксированныйМассив(ПараметрыВыбораДоговор);
	Элементы.Договор.СвязиПараметровВыбора          = Новый ФиксированныйМассив(СвязиПараметровВыбораДоговор);
	
	Элементы.ГруппаФинансовогоУчета.ПараметрыВыбора = Новый ФиксированныйМассив(ПараметрыВыбораГФУ);
	
КонецПроцедуры

&НаКлиентеНаСервереБезКонтекста
Функция ХозяйственнаяОперацияДоговора(ХозяйственнаяОперацияДокумента)
	
	ХозОперация = ПредопределенноеЗначение("Перечисление.ХозяйственныеОперации.ПустаяСсылка");
	
	Если ХозяйственнаяОперацияДокумента =
					ПредопределенноеЗначение("Перечисление.ХозяйственныеОперации.ВыкупПринятыхНаХранениеТоваров") Тогда
		
		ХозОперация = ПредопределенноеЗначение("Перечисление.ХозяйственныеОперации.ПриемНаХранениеСПравомПродажи");
		
	ИначеЕсли ХозяйственнаяОперацияДокумента =
						ПредопределенноеЗначение("Перечисление.ХозяйственныеОперации.ПоставкаПодПринципала") Тогда
		
		ХозОперация = ХозяйственнаяОперацияДокумента;
		
	КонецЕсли;
	
	Возврат ХозОперация;
	
