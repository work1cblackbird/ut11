					<dcssch:field>ЕстьКиЗГИСМ</dcssch:field>
				</Field>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>ЗаказНаВнутреннееПотребление</dcssch:dataPath>
