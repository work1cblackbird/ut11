					<dcssch:field>ПотреблениеДляДеятельности</dcssch:field>
				</Field>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>ИсправляемыйДокумент</dcssch:dataPath>
					<dcssch:field>ИсправляемыйДокумент</dcssch:field>
				</Field>
				<Field xsi:type="dcssch:DataSetFieldField">
