    ШаблонТаблицыСерий.Колонки.Добавить("ПоЧекуКоррекции", Новый ОписаниеТипов("Булево"));
    ШаблонТаблицыСерий.Колонки.Добавить("СпособОпределенияСебестоимости", Новый ОписаниеТипов("ПеречислениеСсылка.СпособыОпределенияСебестоимостиВозврата"));
    ШаблонТаблицыСерий.Колонки.Добавить("ДокументРеализации", Новый ОписаниеТипов("ДокументСсылка.ОтчетОРозничныхПродажах"));

    ШаблонТаблицыТоваров = Новый ТаблицаЗначений;
    ШаблонТаблицыТоваров.Колонки.Добавить("НомерСтроки", Новый ОписаниеТипов("Число"));
    ШаблонТаблицыТоваров.Колонки.Добавить("Номенклатура", Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
    ШаблонТаблицыТоваров.Колонки.Добавить("Характеристика", Новый ОписаниеТипов("СправочникСсылка.ХарактеристикиНоменклатуры"));
    ШаблонТаблицыТоваров.Колонки.Добавить("Количество", Новый ОписаниеТипов("Число"));
    ШаблонТаблицыТоваров.Колонки.Добавить("Помещение", Новый ОписаниеТипов("СправочникСсылка.СкладскиеПомещения"));
    ШаблонТаблицыТоваров.Колонки.Добавить("СтатусУказанияСерий", Новый ОписаниеТипов("Число"));
    ШаблонТаблицыТоваров.Колонки.Добавить("Серия", Новый ОписаниеТипов("СправочникСсылка.СерииНоменклатуры"));    
    ШаблонТаблицыТоваров.Колонки.Добавить("ПоЧекуКоррекции", Новый ОписаниеТипов("Булево"));
    ШаблонТаблицыТоваров.Колонки.Добавить("СпособОпределенияСебестоимости", Новый ОписаниеТипов("ПеречислениеСсылка.СпособыОпределенияСебестоимостиВозврата"));
    ШаблонТаблицыТоваров.Колонки.Добавить("ДокументРеализации", Новый ОписаниеТипов("ДокументСсылка.ОтчетОРозничныхПродажах"));

    МассивСсылокДляОбработки = Новый Массив;
    Если ПолучитьФункциональнуюОпцию("ИспользоватьСерииНоменклатуры") Тогда
        Запрос = Новый Запрос;
        Запрос.Текст =
        "ВЫБРАТЬ
        |	ОтчетОРозничныхВозвратах.Ссылка КАК Ссылка,
        |	ОтчетОРозничныхВозвратах.Дата КАК Дата,
        |	ОтчетОРозничныхВозвратах.Склад КАК Склад
        |ПОМЕСТИТЬ ПредварительнаяТаблицаДокументов
        |ИЗ
        |	Документ.ОтчетОРозничныхВозвратах КАК ОтчетОРозничныхВозвратах
        |ГДЕ
        |	ИСТИНА В
        |		(ВЫБРАТЬ ПЕРВЫЕ 1
        |			ИСТИНА
        |		ИЗ
        |			Документ.ОтчетОРозничныхВозвратах.Товары КАК Товары
        |		ГДЕ
        |			Товары.Ссылка = ОтчетОРозничныхВозвратах.Ссылка
        |			И Товары.СтатусУказанияСерий = 9)
        |
        |ИНДЕКСИРОВАТЬ ПО
        |	Ссылка
        |;
        |
        |////////////////////////////////////////////////////////////////////////////////
        |ВЫБРАТЬ
        |	ТаблицаДокументов.Ссылка КАК Ссылка,
        |	ТаблицаДокументов.Дата КАК Дата,
        |	ТаблицаДокументов.Склад КАК Склад
        |ИЗ
        |	ПредварительнаяТаблицаДокументов КАК ТаблицаДокументов
        |;
        |
        |////////////////////////////////////////////////////////////////////////////////
        |ВЫБРАТЬ
        |	ОтчетОРозничныхВозвратахТовары.Ссылка КАК Ссылка,
        |	ОтчетОРозничныхВозвратахТовары.НомерСтроки КАК НомерСтроки,
        |	ОтчетОРозничныхВозвратахТовары.Номенклатура КАК Номенклатура,
        |	ОтчетОРозничныхВозвратахТовары.Характеристика КАК Характеристика,
        |	ОтчетОРозничныхВозвратахТовары.Количество КАК Количество,
        |	ОтчетОРозничныхВозвратахТовары.СтатусУказанияСерий КАК СтатусУказанияСерий,
        |	ОтчетОРозничныхВозвратахТовары.Помещение КАК Помещение,
        |	ОтчетОРозничныхВозвратахТовары.Серия КАК Серия,
        |	ОтчетОРозничныхВозвратахТовары.ПоЧекуКоррекции КАК ПоЧекуКоррекции,
        |	ОтчетОРозничныхВозвратахТовары.СпособОпределенияСебестоимости КАК СпособОпределенияСебестоимости,
        |	ОтчетОРозничныхВозвратахТовары.ДокументРеализации КАК ДокументРеализации
        |ИЗ
        |	Документ.ОтчетОРозничныхВозвратах.Товары КАК ОтчетОРозничныхВозвратахТовары
        |		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ПредварительнаяТаблицаДокументов КАК ТаблицаДокументов
        |		ПО ОтчетОРозничныхВозвратахТовары.Ссылка = ТаблицаДокументов.Ссылка
        |ГДЕ
        |	ОтчетОРозничныхВозвратахТовары.СтатусУказанияСерий = 9
        |ИТОГИ
        |ПО
        |	Ссылка
        |;
        |
        |////////////////////////////////////////////////////////////////////////////////
        |ВЫБРАТЬ
        |	ОтчетОРозничныхВозвратахСерии.Ссылка КАК Ссылка,
        |	ОтчетОРозничныхВозвратахСерии.Количество КАК Количество,
        |	ОтчетОРозничныхВозвратахСерии.Номенклатура КАК Номенклатура,
        |	ОтчетОРозничныхВозвратахСерии.Характеристика КАК Характеристика,
        |	ОтчетОРозничныхВозвратахСерии.Помещение КАК Помещение,
        |	ОтчетОРозничныхВозвратахСерии.ПоЧекуКоррекции КАК ПоЧекуКоррекции,
        |	ОтчетОРозничныхВозвратахСерии.СпособОпределенияСебестоимости КАК СпособОпределенияСебестоимости,
        |	ОтчетОРозничныхВозвратахСерии.ДокументРеализации КАК ДокументРеализации
        |ИЗ
        |	Документ.ОтчетОРозничныхВозвратах.Серии КАК ОтчетОРозничныхВозвратахСерии
        |		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ПредварительнаяТаблицаДокументов КАК ТаблицаДокументов
        |		ПО ОтчетОРозничныхВозвратахСерии.Ссылка = ТаблицаДокументов.Ссылка
        |ИТОГИ
        |ПО
        |	Ссылка";
        
        РезультатЗапроса = Запрос.ВыполнитьПакет();
        
        ВыборкаДокументов = РезультатЗапроса[1].Выбрать();
        ВыборкаТоваров = РезультатЗапроса[2].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
        ВыборкаСерий = РезультатЗапроса[3].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
        
        Пока ВыборкаДокументов.Следующий() Цикл
            
            СтруктураОбъекта = Новый Структура("Ссылка,Дата,Склад");
            ЗаполнитьЗначенияСвойств(СтруктураОбъекта, ВыборкаДокументов);
            
            СтруктураПоиска = Новый Структура("Ссылка", СтруктураОбъекта.Ссылка);
            
            ВыборкаТоваров.Сбросить();
            Если ВыборкаТоваров.НайтиСледующий(СтруктураПоиска) Тогда
                
                СтрокиТоваров = ШаблонТаблицыТоваров.СкопироватьКолонки();
                
                ВыборкаСтрокиТоваров = ВыборкаТоваров.Выбрать();
                Пока ВыборкаСтрокиТоваров.Следующий() Цикл
                    ЗаполнитьЗначенияСвойств(СтрокиТоваров.Добавить(), ВыборкаСтрокиТоваров);    
                КонецЦикла;
                
                СтрокиСерий = ШаблонТаблицыСерий.СкопироватьКолонки();
                ВыборкаСерий.Сбросить();
                Если ВыборкаСерий.НайтиСледующий(СтруктураПоиска) Тогда
                    ВыборкаСтрокиСерий = ВыборкаСерий.Выбрать();
                    Пока ВыборкаСтрокиСерий.Следующий() Цикл
                        ЗаполнитьЗначенияСвойств(СтрокиСерий.Добавить(), ВыборкаСтрокиСерий);    
                    КонецЦикла;
                КонецЕсли;
                
                ПараметрыУказанияСерий = Новый ФиксированнаяСтруктура(Документы.ОтчетОРозничныхВозвратах.ПараметрыУказанияСерий(СтруктураОбъекта));
                ВыборкаИзЗапросаРасчетаСтатусов = НоменклатураСервер.ВыборкаИзЗапросаРасчетаСтатусовУказанияСерий(ПараметрыУказанияСерий,
                                                                        СтрокиТоваров,
                                                                        СтрокиСерий,
                                                                        СтруктураОбъекта.Склад,
                                                                        Неопределено,
                                                                        Неопределено);
                                                                        
                Если ВыборкаИзЗапросаРасчетаСтатусов.Количество() Тогда
                    МассивСсылокДляОбработки.Добавить(СтруктураОбъекта.Ссылка);
                КонецЕсли;
            КонецЕсли;
            
        КонецЦикла;
        
    КонецЕсли;
    
    ОбновлениеИнформационнойБазы.ОтметитьКОбработке(Параметры, МассивСсылокДляОбработки);
	
КонецПроцедуры

// Обработчик обновления.
// 
// Параметры:
//  Параметры - Структура - параметры.
//
Процедура ОбработатьДанныеДляДляПереходаНаНовуюВерсию(Параметры) Экспорт
	
    ПолноеИмяОбъекта        = ПустаяСсылка().Метаданные().ПолноеИмя();
    
    ОбновляемыеДанные = ОбновлениеИнформационнойБазы.ДанныеДляОбновленияВМногопоточномОбработчике(Параметры);
    
    Для Каждого Документ Из ОбновляемыеДанные Цикл
        
        НачатьТранзакцию();
        
        Попытка
            
            Блокировка = Новый БлокировкаДанных;
            
            ЭлементБлокировки = Блокировка.Добавить(ПолноеИмяОбъекта);
            ЭлементБлокировки.УстановитьЗначение("Ссылка", Документ.Ссылка);
            ЭлементБлокировки.Режим = РежимБлокировкиДанных.Исключительный;
            
            Блокировка.Заблокировать();
            
            ОтчетОРозничныхВозвратахОбъект = Документ.Ссылка.ПолучитьОбъект();
            СтрокиТоваровДляЗаполнения = ОтчетОРозничныхВозвратахОбъект.Товары.НайтиСтроки(Новый Структура("СтатусУказанияСерий", 9));
            
            ПараметрыУказанияСерий = Новый ФиксированнаяСтруктура(Документы.ОтчетОРозничныхВозвратах.ПараметрыУказанияСерий(ОтчетОРозничныхВозвратахОбъект));

            // Выборка строк, по которым необходимо внести изменения
            ВыборкаИзЗапросаРасчетаСтатусов = НоменклатураСервер.ВыборкаИзЗапросаРасчетаСтатусовУказанияСерий(ПараметрыУказанияСерий,
                                                                    ОтчетОРозничныхВозвратахОбъект.Товары,
                                                                    ОтчетОРозничныхВозвратахОбъект.Серии,
                                                                    ОтчетОРозничныхВозвратахОбъект.Склад,
                                                                    СтрокиТоваровДляЗаполнения,
                                                                    Неопределено);
                                                                    
            Пока ВыборкаИзЗапросаРасчетаСтатусов.Следующий() Цикл
                СтрТабл = ОтчетОРозничныхВозвратахОбъект.Товары[ВыборкаИзЗапросаРасчетаСтатусов.НомерСтроки - 1];
                Если ПараметрыУказанияСерий.ИменаПолейСтатусУказанияСерий.Количество() = 0 Тогда
                    СтрТабл.СтатусУказанияСерий = ВыборкаИзЗапросаРасчетаСтатусов.СтатусУказанияСерий;
                Иначе
                    Для Каждого ИмяПоляСтатус Из ПараметрыУказанияСерий.ИменаПолейСтатусУказанияСерий Цикл
                        СтрТабл[ИмяПоляСтатус] = ВыборкаИзЗапросаРасчетаСтатусов[ИмяПоляСтатус];
                    КонецЦикла;
                КонецЕсли;
            КонецЦикла;
            
            Если ОтчетОРозничныхВозвратахОбъект.Модифицированность() Тогда
                ОбновлениеИнформационнойБазы.ЗаписатьДанные(ОтчетОРозничныхВозвратахОбъект);
            Иначе
                ОбновлениеИнформационнойБазы.ОтметитьВыполнениеОбработки(Документ.Ссылка);
            КонецЕсли;
            
            ЗафиксироватьТранзакцию();
            
        Исключение
            
            ОтменитьТранзакцию();
            
            ОбновлениеИнформационнойБазыУТ.СообщитьОНеудачнойОбработке(ИнформацияОбОшибке(), Документ.Ссылка);
            
        КонецПопытки;
        
    КонецЦикла;
    
    Параметры.ОбработкаЗавершена = ОбновлениеИнформационнойБазы.ОбработкаДанныхЗавершена(Параметры.Очередь, ПолноеИмяОбъекта);

КонецПроцедуры

#КонецОбласти

// Возвращает текст запроса для расчета статусов указания серий (С помещениями)
//	Возвращаемое значение:
//		Строка - текст запроса.
//
Функция ТекстЗапросаЗаполненияСтатусовУказанияСерийСПомещениями()

	ТекстЗапроса = 
	"ВЫБРАТЬ
	|	Товары.Помещение,
	|	Товары.Номенклатура,
	|	Товары.Характеристика,
	|	Товары.Серия,
	|	Товары.Количество,
	|	Товары.СтатусУказанияСерий,
	|	Товары.НомерСтроки
	|ПОМЕСТИТЬ Товары
	|ИЗ
	|	&Товары КАК Товары
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Товары.Помещение,
	|	Товары.Номенклатура,
	|	Товары.Характеристика,
	|	СУММА(Товары.Количество) КАК Количество,
	|	ВЫРАЗИТЬ(Товары.Номенклатура КАК Справочник.Номенклатура).ВидНоменклатуры КАК ВидНоменклатуры
	|ПОМЕСТИТЬ ТоварыДляЗапроса
	|ИЗ
	|	Товары КАК Товары
	|
	|СГРУППИРОВАТЬ ПО
	|	Товары.Помещение,
	|	Товары.Номенклатура,
	|	Товары.Характеристика,
	|	ВЫРАЗИТЬ(Товары.Номенклатура КАК Справочник.Номенклатура).ВидНоменклатуры
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Серии.Помещение,
	|	Серии.Номенклатура,
	|	Серии.Характеристика,
	|	Серии.Количество
	|ПОМЕСТИТЬ Серии
	|ИЗ
	|	&Серии КАК Серии
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Серии.Помещение,
	|	Серии.Номенклатура,
	|	Серии.Характеристика,
	|	СУММА(Серии.Количество) КАК Количество
	|ПОМЕСТИТЬ СерииДляЗапроса
	|ИЗ
	|	Серии КАК Серии
	|
	|СГРУППИРОВАТЬ ПО
	|	Серии.Помещение,
	|	Серии.Номенклатура,
	|	Серии.Характеристика
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|		ПолитикиУчетаСерий.Ссылка КАК Ссылка,
	|		ПолитикиУчетаСерий.ПолитикаУчетаСерий КАК ПолитикаУчетаСерий,
	|		ПолитикиУчетаСерий.ПолитикаУчетаСерий.УчитыватьСебестоимостьПоСериям КАК УчитыватьСебестоимостьПоСериям,
	|		ПолитикиУчетаСерий.ПолитикаУчетаСерий.УказыватьПриПланированииОтгрузки КАК УказыватьПриПланированииОтгрузки,
	|		ПолитикиУчетаСерий.ПолитикаУчетаСерий.УказыватьПриПланированииОтбора КАК УказыватьПриПланированииОтбора,
	|		ПолитикиУчетаСерий.ПолитикаУчетаСерий.УчетСерийПоFEFO КАК УчетСерийПоFEFO,
	|		ПолитикиУчетаСерий.ПолитикаУчетаСерий.УказыватьПоФактуОтбора КАК УказыватьПоФактуОтбора,
	|		ПолитикиУчетаСерий.ПолитикаУчетаСерий.УказыватьПриОтгрузкеВРозницу КАК УказыватьПриОтгрузкеВРозницу,
	|		ПолитикиУчетаСерий.ПолитикаУчетаСерий.УчитыватьОстаткиСерий КАК УчитыватьОстаткиСерий
	|ПОМЕСТИТЬ ПолитикиУчетаСерийДляЗапроса
	|ИЗ
	|	Справочник.ВидыНоменклатуры.ПолитикиУчетаСерий КАК ПолитикиУчетаСерий
	|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ ТоварыДляЗапроса КАК ТоварыДляЗапроса
	|		ПО ТоварыДляЗапроса.ВидНоменклатуры = ПолитикиУчетаСерий.Ссылка
	|ГДЕ 
	|	(ПолитикиУчетаСерий.Склад = &Склад)	
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Товары.НомерСтроки КАК НомерСтроки,
	|	Товары.СтатусУказанияСерий КАК СтарыйСтатусУказанияСерий,
	|	ВЫБОР
	|		КОГДА ПолитикиУчетаСерий.ПолитикаУчетаСерий ЕСТЬ NULL 
	|			ТОГДА 0
	|		КОГДА ПолитикиУчетаСерий.УчитыватьСебестоимостьПоСериям
	|			ТОГДА ВЫБОР
	|					КОГДА Товары.Серия <> ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка)
	|						ТОГДА 14
	|					ИНАЧЕ 13
	|				КОНЕЦ
	|		КОГДА &ТолькоСерииДляСебестоимости
	|			ТОГДА 0
	|		КОГДА ПолитикиУчетаСерий.УказыватьПриПланированииОтгрузки
