		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Инциденты.Ссылка КАК Ссылка,
	Инциденты.Подразделение КАК Подразделение,
	Инциденты.Исполнитель КАК Исполнитель,
	Инциденты.ВидИнцидента КАК ВидИнцидента,
	Инциденты.Критичность КАК Критичность,
	Инциденты.Статус КАК Статус,
	Инциденты.Автор КАК Автор,
	ВЫБОР
		КОГДА Инциденты.Дубль &lt;&gt; ЗНАЧЕНИЕ(Справочник.Инциденты.ПустаяСсылка)
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Дубль,
	Инциденты.ЖелаемаяДатаЗакрытия КАК ЖелаемаяДатаЗакрытия,
	Инциденты.ДатаЗакрытия КАК ДатаЗакрытия,
	1 КАК Количество,
	ВЫБОР
		КОГДА Инциденты.ЖелаемаяДатаЗакрытия = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
			ТОГДА ЛОЖЬ
		ИНАЧЕ ВЫБОР
				КОГДА Инциденты.ДатаЗакрытия = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
					ТОГДА ВЫБОР
							КОГДА РАЗНОСТЬДАТ(НАЧАЛОПЕРИОДА(Инциденты.ЖелаемаяДатаЗакрытия, ДЕНЬ), НАЧАЛОПЕРИОДА(&amp;ТекущаяДата, ДЕНЬ), ДЕНЬ) &gt; 0
								ТОГДА ИСТИНА
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
				ИНАЧЕ ВЫБОР
						КОГДА РАЗНОСТЬДАТ(Инциденты.ЖелаемаяДатаЗакрытия, Инциденты.ДатаЗакрытия, ДЕНЬ) &gt; 0
							ТОГДА ИСТИНА
						ИНАЧЕ ЛОЖЬ
					КОНЕЦ
			КОНЕЦ
	КОНЕЦ КАК Просрочен,
	ВЫБОР
		КОГДА Инциденты.ЖелаемаяДатаЗакрытия = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА Инциденты.ДатаЗакрытия = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
					ТОГДА ВЫБОР
							КОГДА НАЧАЛОПЕРИОДА(Инциденты.ЖелаемаяДатаЗакрытия, ДЕНЬ) &lt; НАЧАЛОПЕРИОДА(&amp;ТекущаяДата, ДЕНЬ)
								ТОГДА РАЗНОСТЬДАТ(НАЧАЛОПЕРИОДА(Инциденты.ЖелаемаяДатаЗакрытия, ДЕНЬ), НАЧАЛОПЕРИОДА(&amp;ТекущаяДата, ДЕНЬ), ДЕНЬ)
							ИНАЧЕ 0
						КОНЕЦ
				ИНАЧЕ ВЫБОР
						КОГДА Инциденты.ЖелаемаяДатаЗакрытия &lt; Инциденты.ДатаЗакрытия
							ТОГДА РАЗНОСТЬДАТ(Инциденты.ЖелаемаяДатаЗакрытия, Инциденты.ДатаЗакрытия, ДЕНЬ)
						ИНАЧЕ 0
					КОНЕЦ
			КОНЕЦ
	КОНЕЦ КАК ПросроченоДней
ИЗ
	Справочник.Инциденты КАК Инциденты
ГДЕ
	Инциденты.ПометкаУдаления = ЛОЖЬ
{ГДЕ
	(Инциденты.ДатаРегистрации &gt;= &amp;ДатаНачала),
	(Инциденты.ДатаРегистрации &lt;= &amp;ДатаОкончания)}</query>
		<useQueryGroupIfPossible>false</useQueryGroupIfPossible>
	</dataSet>
