<script>
    var app = new Vue({
        el: '#app',
        data: {
            area: {},
            allowed_min_zoom: 1,
            canvas: null,
            message: 'Hello Vue!',
            showImage: true,
            image_src: null,
            img_html: null,
            cropMode: false,
            images: [],
            full_height: 0,
            max_width: 0,
            canvas_back: null,
            cropStarted: false,
            pressTimer: null,
            canvas_width: window.innerWidth,
            scale_factor: -1,
            scale_x: 1,
            max_y: 0,
            scale_y: 1,
            refresh: 0,
            mainRect: {},
            cropRect: null,
            bbox: null,
            is: null,
            not_printable: false,
            zoomChanged: false,
            image_group: new fabric.Group([], {
                left: 0, top: 0, lockMovementX: true,
                lockMovementY: true,
                selectable: false
            }),
            image: null
        },
        methods: {
            save: function (el) {
                let link = document.getElementById("img_link");
                link.href = this.image_src; // this.canvas.toDataURL();
                link.download = "document.jpg";
                link.click();


                //let img = this.canvas.toDataURL("image/png");
                // el.href = img;
                //window.open(this.canvas.toDataURL('png'));
                //var dataURL = canvas.toDataURL('image/png');
                // button.href = dataURL;
            },
            changeCropMode: function () {
                this.cropMode = !this.cropMode;
                if (!this.cropMode) {
                    this.removeCrop();
                } else {

                }
            },
            getScaleWithZoom: function () {
                let zoom = this.canvas.getZoom();
                return zoom * window.innerWidth / this.max_width;
            },
            prevent_going_out: function () {
                let main = this;
                let zoom = main.canvas.getZoom();
                let vpt = main.canvas.viewportTransform;
                let left = (1 - zoom) * window.innerWidth / 2
                if (zoom < 1) {
                    main.image.left = left / zoom;
                    // main.image_group.left = left / zoom;
                } else {
                    main.image.left = 0;
                    // main.image_group.left = 0;
                }
                if (vpt[4] > 0 || left > 0) {
                    // console.log('null')
                    vpt[4] = 0;
                }
                if (vpt[5] > 0) {
                    vpt[5] = 0;
                }

                let right_point = -this.canvas.viewportTransform[4] / this.getScaleWithZoom() + this.canvas.width / this.getScaleWithZoom()

                if (right_point > this.max_width && vpt[4] != 0) {
                    this.canvas.viewportTransform[4] = (-this.max_width + this.canvas.width / this.getScaleWithZoom()) * this.getScaleWithZoom()
                    // this.canvas.viewportTransform[4] = (right_point - this.max_width) * this.getScaleWithZoom()
                }

                if ((main.canvas.height - vpt[5]) / vpt[0] > main.max_y) {
                    vpt[5] = main.canvas.height - vpt[0] * main.max_y;
                }
                if (vpt[5] > 0) {
                    vpt[5] = 0;
                }
                main.canvas.renderAll();
                // console.log('vpt4-5', vpt[4], vpt[5])
                let bugMustBeFixed = false;
                if (!main.canvas.item(0).isOnScreen())
                    bugMustBeFixed = true;

                if (bugMustBeFixed)
                    main.canvas.item(0).setCoords();
                if (main.canvas.item(0).isOnScreen()) {
                    main.canvas.renderAll();
                }

            },
            zoom: function (opt, sign, toDefault) {
                let main = this;
                sign = (sign == null) ? -1 : sign;
                toDefault = (toDefault == null) ? false : toDefault;
                let defaultDelta = sign * Math.trunc(window.innerHeight * 0.05);
                let defaultOffsetY = Math.trunc(window.innerHeight / 2);
                let defaultOffsetX = Math.trunc(window.innerWidth / 2);
                let delta = (opt != null) ? opt.e.deltaY : defaultDelta;
                let min_zoom = window.innerHeight / main.full_height;
                // var pointer = area.canvas.getPointer(opt.e);
                if (min_zoom >= 1) {
                    min_zoom = 1;
                }
                let zoom = main.canvas.getZoom();
                let oldZoom = zoom;
                zoom = zoom * Math.pow(1 - Math.sign(delta) * 0.1, 2)//Math.log(1.2 * zoom);
                if (zoom > 6.5) zoom = 6.5;
                if (zoom < min_zoom) zoom = min_zoom;
                if (toDefault) zoom = 1;
                if (!main.zoomChanged) {
                    main.zoomChanged = oldZoom != zoom;
                }
                main.canvas.zoomToPoint({
                    x: (opt != null) ? opt.e.offsetX : defaultOffsetX,
                    y: (opt != null) ? opt.e.offsetY : defaultOffsetY
                }, zoom);
                //alert(zoom)
                main.user_zoom = zoom;
                if (opt != null) {
                    opt.e.preventDefault();
                    opt.e.stopPropagation();
                }
                main.prevent_going_out();
                if (main.bbox) {
                    zoom_to_bbox(main.bbox.x0, main.bbox.y0, main.bbox.x1, main.bbox.y1, 0, null, 0, true);
                }
                main.canvas.renderAll();
            },
            removeCrop: function () {
                if (this.cropRect) {
                    console.log('remove crop')
                    this.canvas.remove(this.cropRect);
                    this.area.cropRect = null;
                }
            },
            clearMove: function () {
                clearTimeout(this.pressTimer);
                return false;


            },

            move: function (direction) {
                let main = this;
                main.move_simple(direction);
                this.pressTimer = setInterval(function () {
                    main.move_simple(direction);
                }, 200);
                return false;
            },

            move_simple: function (direction) {

                // var zoom = area.canvas.getZoom();
                // this.getContext("2d").translate(e.clientX - this.lastPosX, e.clientY - this.lastPosY);
                // leftX += (e.clientX - this.lastPosX) / zoom;
                // topY +=  (e.clientY - this.lastPosY) / zoom;
                if (direction == 'left') {
                    this.canvas.viewportTransform[4] += Math.round(window.innerWidth * 0.05);

                } else if (direction == 'up') {
                    this.canvas.viewportTransform[5] += Math.round(window.innerHeight * 0.05);
                } else if (direction == 'down') {
                    this.canvas.viewportTransform[5] -= Math.round(window.innerHeight * 0.05);
                } else if (direction == 'right') {
                    this.canvas.viewportTransform[4] -= Math.round(window.innerWidth * 0.05);
                }
                // this.area.canvas.viewportTransform[5] += e.clientY - this.lastPosY;

                //this.area.canvas.lastPosX = e.clientX;
                //this.area.canvas.lastPosY = e.clientY;
                this.canvas.item(0).setCoords();
                this.canvas.requestRenderAll();
                this.prevent_going_out();

                this.canvas.renderAll();
                this.canvas.item(0).setCoords();
            },
            set_all_images: function () {
                let main = this;
                main.max_width = 0;

                main.max_width = Math.max.apply(Math, main.images.map(function (o) {
                    return o.width;
                }));
                main.full_height = 0
                main.images.forEach(function (img) {
                    img.scale_factor = window.innerWidth / img.width;
                    // console.log(img.width, img.height, img.scale_factor);
                    main.full_height += img.height * img.scale_factor
                })

                main.canvas.clear();
                main.canvas_back.clear()
                main.max_y = 0;

                // main.init_canvas(window.innerWidth, window.innerHeight)
                main.canvas.setWidth(window.innerWidth)
                main.canvas.setHeight(window.innerHeight)


                main.canvas_width = window.innerWidth
                /*
                main.image_group = new fabric.Group([], {
                    left: 0, top: 0, lockMovementX: true,
                    lockMovementY: true,
                    selectable: false
                })
                */


                // main.scale_factor = (main.scale_factor == -1) ? scale_factor : main.scale_factor;
                // console.log(main.canvas_width / base_image.orig_w, window.innerHeight / base_image.orig_h);

                main.images.forEach(function (img) {
                    let scale_factor = img.scale_factor //* window.innerHeight / main.full_height;
                    let bi = new fabric.Image(img, {
                        left: 0,
                        top: main.max_y,
                        scaleX: scale_factor,
                        scaleY: scale_factor,
                        lockMovementX: true,
                        lockMovementY: true,
                        selectable: false,
                        orig_w: img.orig_w

                    })
                    main.max_y += bi.height * scale_factor;
                    // console.log(main.max_y);
                    // main.area.scale_x = main.area.width / main.area.img_orig_w;
                    // main.area.scale_y = main.area.height / main.area.img_orig_h;
                    // main.image_group.addWithUpdate(bi)
                    main.image = bi;
                    main.canvas.add(bi);
                    document.getElementById('image_load_button').click()
                    // main.canvas.requestRenderAll();
                    // main.prevent_going_out();
                    // main.canvas.renderAll();

                });
                let left = 0; //(window.innerWidth - main.max_width * window.innerHeight / main.full_height) / 2;
                main.image.left = 0;
                // main.image_group.left = 0;
                /*
                main.image_group.getObjects().forEach(function(obj){
                    main.max_y += obj.height;
                })*/


                // main.canvas.add(main.image_group);
                // console.log(full_group.width, full_group.height)


                main.canvas.renderAll();
                main.canvas_back.renderAll();
            },
            init_canvas: function (width, height) {
                console.log("init_canvas");
                let main = this;
                main.canvas = new fabric.Canvas('c')
                main.canvas.setHeight(height)
                main.canvas.setWidth(width);
                main.canvas_back = new fabric.Canvas('c_back')

                main.canvas.on('mouse:down', function (opt) {
                    let evt = opt.e;

                    //this.isDragging = true;
                    //this.selection = false;
                    this.lastPosX = evt.clientX;
                    this.lastPosY = evt.clientY;
                    // return;
                    if (main.cropMode) {
                        main.removeCrop();
                        main.mainRect.visible = true;
                        main.cropStarted = true;
                        // console.log(opt.e.clientX, opt.e.clientY);

                        let circle = new fabric.Circle({
                            radius: 50,
                            fill: 'white',
                            backgroundColor: 'blue',
                            left: opt.e.clientX,
                            top: opt.e.clientY
                        });

                        // Render the circle in canvas
                        // main.canvas.add(circle);

                        main.mainRect.left = opt.e.clientX;
                        main.mainRect.top = opt.e.clientY;
                    } else {
                        console.log("mouse:down");
                        let evt = opt.e;

                        this.isDragging = true;
                        this.selection = false;
                        this.lastPosX = evt.clientX;
                        this.lastPosY = evt.clientY;
                    }
                });

                main.canvas.on('mouse:move', function (opt) {
                    if (this.isDragging && !main.cropStarted) {
                        let e = opt.e;
                        this.viewportTransform[4] += e.clientX - this.lastPosX;
                        this.viewportTransform[5] += e.clientY - this.lastPosY;
                        this.requestRenderAll();
                        this.lastPosX = e.clientX;
                        this.lastPosY = e.clientY;
                        main.prevent_going_out();
                    }
                    if (this.isDragging && main.cropStarted) {
                        let r = main.mainRect;
                        let width = Math.abs(main.mainRect.left - opt.e.clientX);
                        let height = Math.abs(main.mainRect.top - opt.e.clientY);
                        if (r.left > opt.e.clientX) {
                            r.left = opt.e.clientX;
                        }
                        if (r.top > opt.e.clientX) {
                            r.top = opt.e.clientX;
                        }

                        r.width = width;
                        r.height = height;
                        main.canvas.renderAll();
                        main.canvas.requestRenderAll();

                    }
                });

                main.canvas.on('mouse:up', function (opt) {
                    // console.log(opt.e.clientX, opt.e.clientY);

                    if (main.cropStarted) {
                        // main.mainRect.visible = false;
                        let mouseY = opt.e.clientY;
                        let mouseX = opt.e.clientX;
                        // console.log('clientX', opt.e.clientX);
                        let matrix = this.viewportTransform;
                        let left = (main.mainRect.left - matrix[4]) / matrix[0];
                        let top = (main.mainRect.top - matrix[5]) / matrix[0];
                        // let left = main.mainRect.left
                        //let top = main.mainRect.top
                        let right = (mouseX - matrix[4]) / matrix[0];
                        let bottom = (mouseY - matrix[5]) / matrix[3];
                        let top_index = 0;
                        /*
                        main.image_group.getObjects().forEach(function(img){
                            console.log(img.height);
                            if (top > img.height){
                                top_index += 1;
                                top = top - img.height;
                            }else{
                                top -=
                            }
                        })
                        */
                        /*
                        let orig_left = left * main.area.img_orig_w / main.area.img_w
                        let orig_right = right * main.area.img_orig_w / main.area.img_w
                        let orig_top = top * main.area.img_orig_h / main.area.img_h
                        let orig_bottom = bottom * main.area.img_orig_h / main.area.img_h

                        console.log(orig_left, orig_top, orig_right, orig_bottom);
                        */

                        main.cropStarted = false;
                        /*
                        let ctx = main.canvas.getContext("2d");
                        ctx.beginPath();
                        ctx.rect(20, 20, 150, 100);
                        ctx.stroke();*/
                        let r = main.mainRect;
                        let width = Math.abs(left - right);
                        let height = Math.abs(top - bottom);
                        r.left = (left > right) ? right : left;
                        r.top = (top > bottom) ? bottom : top;


                        let rect = new fabric.Rect({
                            top: main.mainRect.top,
                            left: main.mainRect.left,
                            width: width,
                            height: height,
                            fill: 'rgba(255,0,0,0.1)',
                            stroke: '#AA0000',
                            strokeWidth: 1,
                            lockMovementX: true,
                            lockMovementY: true,
                            selectable: false
                        });
                        main.canvas.add(rect);
                        if (main.bbox) {
                            main.canvas.remove(main.bbox);
                            main.bbox = null;
                        }
                        main.cropRect = rect;
                        main.canvas.renderAll();
                        main.canvas.requestRenderAll();


                    } else {
                        this.isDragging = false;
                        this.selection = true;
                    }
                });
                main.canvas.on('mouse:wheel', main.zoom);


                return;

            },
            sleep: function (ms) {
                return new Promise(resolve => setTimeout(resolve, ms));
            },

            resizeWindow: function () {
                let main = this;
                // console.log(window.innerWidth);
                // console.log(window.innerHeight);
                let bbox_x0;
                let bbox_x1;
                let bbox_y0;
                let bbox_y1;
                let bbox;
                main.set_all_images()
                main.prevent_going_out()
                if (main.bbox) {
                    zoom_to_bbox(main.bbox.x0, main.bbox.y0, main.bbox.x1, main.bbox.y1, 0, null, 0, true);
                }
                main.canvas.renderAll()

            },
            add_image: function (image_src) {
                let main = this;
                // console.log('src', image_src)
                this.image_src = image_src;
                this.img_html = '<img src=' + image_src + ' style="width: 100% !important">';
                this.refresh += 1

                let base_image = new Image();

                //main.sleep(1000).then(function (res) {
                base_image.src = image_src; // 'http://localhost:8083/1_api_top.jpg';
                base_image.onload = function () {
                    main.images.push(base_image);
                    main.set_all_images();
                }


                // add_image
                // main.canvas.requestRenderAll()

                //context.drawImage(base_image, 0, 0, window.innerWidth, base_image.height * base_image.width / window.innerWidth);


            },


        },
        mounted: function () {
            let main = this;
            main.init_canvas(window.innerWidth, window.innerHeight)
            // main.canvas.add(main.image_group);
            let sleep = m => new Promise(r => setTimeout(r, m))
            window.addEventListener("resize", main.resizeWindow);

            window.onbeforeprint = function () {
                console.log(main.canvas_back.width, main.canvas_back.height)
            };
            window.addEventListener("afterprint", function () {
                console.log('afterprint');
                //main.canvas.height = window.innerHeight;
                //main.canvas.renderAll();
                //main.not_printable = false;
            });
            let standardScrollbarStyle = document.getElementById('1C_scrollbar_12704CA4-9C01-461B-8383-F4CD6283CB75')
            if (standardScrollbarStyle !== null) {
                standardScrollbarStyle.remove()
