    function zoom_to_bbox(x0, y0, x1, y1, num_of_lines, color, image_index, ignore_zoom) {
        image_index = (image_index) ? image_index : 0
        color = (color) ? color : 'red'
        let current_zoom = app.canvas.getZoom()
        let viewZoneRealWidth = window.innerWidth / app.getScaleWithZoom();
        let viewZoneRealHeight = window.innerHeight / app.getScaleWithZoom();
        let bboxCanFit = (Math.abs(x0 - x1) < viewZoneRealWidth) && (Math.abs(y0 - y1) < viewZoneRealHeight)
        // console.log(viewZoneRealWidth, Math.abs(x0 - x1) > viewZoneRealWidth, x0-x1);
        let dest_zoom = current_zoom;
        let current_left = -app.canvas.viewportTransform[4] / app.getScaleWithZoom()
        let current_top = -app.canvas.viewportTransform[5] / app.getScaleWithZoom()
        let current_right = current_left + viewZoneRealWidth;
        let current_bottom = current_top + viewZoneRealHeight;
        // console.log('current', current_left, current_top, current_right, current_bottom);
        // console.log('target', x0, x1, y0, y1);
        let bboxInView = (current_left < Math.min(x0, x1) && Math.max(x0, x1) < current_right && current_top < Math.min(y0, y1) && Math.max(y0, y1) < current_bottom)
        // console.log(current_x, current_y)

        if (!app.zoomChanged || !bboxCanFit || !bboxInView) {
            dest_zoom = Math.min(window.innerWidth / Math.abs(x0 - x1), window.innerHeight / Math.abs(y0 - y1))
            // zoom_x = (zoom_x * 0.3 > 6.5) ? 6.5 : zoom_x * 0.3;
            dest_zoom = (dest_zoom * 0.8 > 6.5) ? 6.5 : dest_zoom * 0.8;
            dest_zoom = (dest_zoom * 0.8 < 1) ? 1 : dest_zoom * 0.8;
        }
        // dest_zoom = 2
        // x0 += ;
        // x1 += app.image_group.left;
        let scale = app.image.scaleX; //app.image_group.getObjects()[image_index].scaleX;
        let add_y = 0
        for (let i = 0; i < image_index; i++) {
            add_y += app.images[i].height  // * app.image_group.getObjects()[i].scaleX
        }
        y0 += add_y
        y1 += add_y
        let need_move_x = true;
        let need_move_y = true;
        if (!ignore_zoom && (!bboxCanFit || !bboxInView)) {
            if (bboxCanFit) {
                dest_zoom = current_zoom;
            }
            app.canvas.viewportTransform[4] = -((x0 + x1) / 2) * scale * dest_zoom + window.innerWidth / 2;
            app.canvas.viewportTransform[5] = -((y0 + y1) / 2) * scale * dest_zoom + window.innerHeight / 2;

            app.canvas.viewportTransform[0] = dest_zoom;
            app.canvas.viewportTransform[3] = dest_zoom;

            app.prevent_going_out();
        }
        let rect = new fabric.Rect({
            top: y0 * app.image.scaleX, //app.image_group.getObjects()[0].scaleX,
            left: x0 * app.image.scaleX + app.image.left, // app.image_group.getObjects()[0].scaleX + app.image_group.left,
            width: Math.abs(x1 - x0) * scale,
            height: Math.abs(y1 - y0) * scale,
            fill: null,
            // fill: (is_error) ? 'rgba(255, 0, 0, 0.2)' : 'rgba(248,207,48,0.2)',
            stroke: color,
            strokeWidth: 2,
            lockMovementX: true,
            lockMovementY: true,
            selectable: false,
            x0: x0,
            x1: x1,
            y0: y0,
            y1: y1
        });
        if (app.bbox) {
            app.canvas.remove(app.bbox);
        }
        app.bbox = rect;
        app.canvas.add(rect)
        if (!ignore_zoom) {
            app.canvas.requestRenderAll();
            app.canvas.renderAll();
        }
        return;
    }

